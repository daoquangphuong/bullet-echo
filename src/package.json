{
  "name": "bullet-echo",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "12.22.7",
    "yarn": "1.22.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "NODE_ENV=production lint-staged"
    }
  },
  "lint-staged": {
    "src/*": [
      "pretty-quick --staged"
    ],
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "fs-extra": "^10.1.0",
    "joi": "^17.6.0",
    "source-map-support": "^0.5.20"
  }
}
