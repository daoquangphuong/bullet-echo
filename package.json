{
  "name": "bullet-echo",
  "version": "1.0.0",
  "private": true,
  "engines": {},
  "husky": {
    "hooks": {
      "pre-commit": "NODE_ENV=production lint-staged"
    }
  },
  "lint-staged": {
    "src/*": [
      "pretty-quick --staged"
    ],
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "start": "node --inspect=7071 --optimize_for_size --max_old_space_size=460 src/index.js",
    "dev": "nodemon src/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0"
  }
}
